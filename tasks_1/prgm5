//5. Generate a Upper Triangle Matrix of row number with the number of rows and columns as given by the user.
#include <stdio.h>

int main() {
    int i,j, rows;
    printf("Enter the number of rows: ");
    scanf("%d", &rows);
    for ( i =1; i<=rows; i++) 
    {
        for ( j =rows;j >=i;j--) 
        {
             printf("%d", i);
        }
        printf("\n");
    }

    return 0;
}

//add 15 to every element of array
# include <stdio.h>
int main (){
int array[5];
int size;
printf("enter the size of the array :");
scanf("%d",&size);
printf("enter the elements of the array:");
for(int i=0;i<size;i++)
{
scanf("%d",&array[i]);
}
for(int i=0;i<size;i++){
array[i]+=15;
}
printf("upadated array is :")
for(int i =0;i<size;i++){
printf("%d",array[i]);
}
printf("\n");
return 0
}

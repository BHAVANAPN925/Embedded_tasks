#include <stdio.h>

int main() {
    int a[5]={34,23,56,98,87},i,even=0,odd=0;
    for(i=0;i<5;i++)
    {
        if (a[i] % 2 == 0)
        even = even+a[i];
        else
        odd = odd+a[i];
        
    }
    printf("sum of evevn numbers:%d\n",even);
    printf("sum of odd numbers:%d\n",odd);
    return 0;
}

//find an element index in an array 
#include <stdio.h>
int main(){
int array [10];
int size;
int elemnet;
printf("enter the size of an array:");
scanf("%d",&size);
printf("enter the elements:");
for(int i=0;i<size;i++){
scnaf("%d",&array[i]);
}
printf("enter the element to find:");
scanf("%d",&element) ;
int index =-1;
for(int i =0;i<size of (array)/size of (array[0]);i++){
if(array[i]==element){
index= i;
break;
}
}
if(index!=-1){
printf("the element %d is at index %d\n",element,index);
}
else{
printf("the element not found");
}
return 0;
}


